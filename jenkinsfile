node {
	stage('SCM') {
		echo 'Gathering code from version control'
		git branch: '${branch}', url: 'https://github.com/PhilippKleppmann/jenkins-playground.git'
	}
	stage('Validate') {
		echo 'Validating....'
	}
	stage('Build') {
		echo 'Building....'
	}
	stage('Test') {
		echo 'Testing....'
	}
	stage('Deploy') {
		echo 'Deploying....'
	}
}
